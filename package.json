{
  "name": "mcp-appsignal",
  "version": "1.0.0",
  "description": "MCP server for AppSignal integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --skipLibCheck --noEmitOnError",
    "start": "node dist/index.js",
    "start:http": "node dist/http-server.js",
    "start:direct": "node src/index.js",
    "dev": "tsc -w --skipLibCheck --noEmitOnError & node --watch dist/index.js",
    "dev:http": "tsc -w --skipLibCheck --noEmitOnError & node --watch dist/http-server.js",
    "test": "echo \"Tests temporarily disabled\" && exit 0",
    "test:watch": "echo \"Tests temporarily disabled\" && exit 0"
  },
  "keywords": [
    "mcp",
    "appsignal",
    "cursor"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.3",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
